{
  "name": "madge",
  "version": "0.5.4",
  "author": "Patrik Henningsson <patrik.henningsson@gmail.com>",
  "repository": "git://github.com/pahen/madge",
  "homepage": "https://github.com/pahen/madge",
  "license": "MIT",
  "description": "Create graphs from your CommonJS, AMD or ES6 module dependencies.",
  "keywords": [
    "ES6",
    "ES7",
    "AMD",
    "RequireJS",
    "require",
    "module",
    "circular",
    "dependency",
    "dependencies",
    "graphviz",
    "graph"
  ],
  "engines": [
    "node >= 0.8.0"
  ],
  "scripts": {
    "test": "npm run lint && npm run mocha && npm run madge",
    "mocha": "mocha test/*.js",
    "lint": "eslint bin lib",
    "madge": "madge -c -f cjs ./lib",
    "release": "npm run ci && release-it -n -i patch",
    "release:minor": "npm run ci && release-it -n -i minor",
    "release:major": "npm run ci && release-it -n -i major"
  },
  "dependencies": {
    "amdetective": "~0.2.1",
    "coffee-script": "1.3.3",
    "colors": "^1.1.2",
    "commander": "^2.8.1",
    "commondir": "^1.0.1",
    "detective": "^4.2.0",
    "detective-es6": "1.1.5",
    "graphviz": "0.0.8",
    "resolve": "^1.1.6",
    "uglify-js": "^1.3.5",
    "walkdir": "0.0.10"
  },
  "devDependencies": {
    "@aptoma/eslint-config": "^4.0.0",
    "eslint": "^3.0.0",
    "mocha": "^2.3.3",
    "should": "*",
    "release-it": "^2.4.0"
  },
  "main": "./lib/madge",
  "bin": {
    "madge": "./bin/madge"
  }
}
